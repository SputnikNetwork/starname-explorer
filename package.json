{
  "name": "starname_scrapper",
  "version": "1.0.0",
  "author": "Albert Andrejev",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "dry-build": "tsc --noEmit",
    "start": "yarn process-genesis && yarn process-live",
    "process-live": "ts-node src/index.ts",
    "process-genesis": "ts-node src/process-genesis.ts",
    "format": "prettier --write --loglevel warn './src/**/*.{ts,json,md,css}'",
    "lint": "eslint -c .eslintrc.js --max-warnings 0 'src/**/*.ts'",
    "lint:fix": "eslint -c .eslintrc.js 'src/**/*.ts' --fix"
  },
  "dependencies": {
    "@cosmjs/launchpad": "^0.23.1",
    "@cosmjs/socket": "^0.23.1",
    "dotenv": "^8.2.0",
    "mongoose": "^5.10.13",
    "stream-chain": "^2.2.3",
    "stream-json": "^1.7.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/mongoose": "^5.10.0",
    "@types/stream-chain": "^2.0.0",
    "@types/stream-json": "^1.5.1",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "eslint": "7.10.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "prettier": "^2.1.2"
  }
}
